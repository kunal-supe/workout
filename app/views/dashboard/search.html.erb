<div class="row search-result">
  <% @atheletes.each do |athelete| %>
  <div class="col-md-10 col-xs-offset-1 well member">
    <%= athelete.full_name %>
    <% if current_user %>
      <%= link_to "Follow", friendships_path(friend_id: athelete.id), method: :post,
            class: "btn btn-primary pull-right" unless
            current_user.follows_or_same?(athelete) %>
    <% end %>
  </div>
  <% end %>
<%= will_paginate @atheletes, renderer: BootstrapPagination::Rails %>
</div>
